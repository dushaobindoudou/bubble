{
  "name": "bubble-brawl-client",
  "version": "1.0.0",
  "description": "Bubble Brawl Game Client with React + Vite",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-types": "tsc && vite build",
    "preview": "vite preview",
    "serve": "vite",
    "type-check": "tsc --noEmit",
    "game-server": "node test-game-server.cjs",
    "dev:full": "concurrently \"npm run game-server\" \"npm run dev\""
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^1.3.0",
    "@tanstack/react-query": "^4.36.0",
    "clsx": "^2.0.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "ethers": "^6.8.0",
    "express": "^5.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.8.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "viem": "^1.19.0",
    "wagmi": "^1.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
